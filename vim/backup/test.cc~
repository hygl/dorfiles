#include <queue>
#include <map>
#include <string>
#include <iostream>
struct ts {
  int num;
  std::string name;
};


std::map<std::string,std::queue<ts> >bwqueue;
//std::map<std::string,double> bwsize;
main()
{
  while(true)
	{
	  ts mts;
	  mts.num =1;
	  mts.name ="test";
	  if(bwqueue.end() == bwqueue.find("test"))
		{
		
		std::queue<ts> mycue;
		mycue.push(mts);
		std::cout << mycue.size();
		bwqueue["test"] = mycue;
		}
	  std::map<std::string,std::queue<ts> >::iterator it = bwqueue.find("test");
	  (*it).second.push(mts);
	  std::cout << (*it).second.size();
	  (*it).second.pop();
	  std::cout << (*it).second.size();
	}
};
